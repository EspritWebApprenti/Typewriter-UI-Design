const text2 = document.querySelector('.text2');

const changement = () => {
    setTimeout(() => {
        text2.textContent = "Etudiant";
    }, 0);
    setTimeout(() => {
        text2.textContent = "YouTubeur";
    }, 3000);
    setTimeout(() => {
        text2.textContent = "DÃ©butant";
    }, 6000)
}

changement();
setInterval(changement, 9000)
